<div class="container well" id="inspect-apply-index">
  <div class="row">
    <form class="form-inline">
      {{#inline-form-field}}病人ID：{{/inline-form-field}}
      {{#inline-form-field}}身份证号：{{/inline-form-field}}
      {{#inline-form-field}}姓名：{{/inline-form-field}}
      {{#inline-form-field}}检查单号：{{/inline-form-field}}
      <div class="form-group">
        <label>信息来源:</label>
        <select class="form-control input-sm">
          <option>来源一</option>
          <option>来源二</option>
          <option>来源三</option>
          <option>来源四</option>
        </select>
      </div>
      <div class="form-group">
        <label>状态:</label>
        <select class="form-control input-sm">
          <option>已开单</option>
          <option>已报到</option>
          <option>采集中</option>
          <option>已回收</option>
        </select>
      </div>
      <div class="form-group">
        <label>送检科室:</label>
        <select class="form-control input-sm">
          <option>心外科</option>
          <option>神经外科</option>
          <option>妇产科</option>
        </select>
      </div>
      <div class="form-group">
        <label>检查项目:</label>
        <select class="form-control input-sm">
          <option>静息心电图</option>
          <option>动态心电图</option>
          <option>床旁心电图</option>
        </select>
      </div>
      <div class="form-group">
        <label class="control-label">申请时间</label>
        {{date-picker date=model.birthday valueFormat='YYYY-MM-DD' class="form-control input-sm"}}
      </div>
      <div class="form-group">
        <label class="control-label">-</label>
        {{date-picker date=model.birthday valueFormat='YYYY-MM-DD' class="form-control input-sm"}}
      </div>
      <button class="btn btn-primary">搜索</button>
    </form>
  </div>
